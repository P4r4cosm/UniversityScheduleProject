{
    "name": "redis-sink",
    "config": {
        "connector.class": "com.datamountaineer.streamreactor.connect.redis.sink.RedisSinkConnector",
        "tasks.max": "1",
        "topics": "postgres.public.students,postgres.public.courses",
        "redis.host": "redis",
        "redis.port": "6379",
        "redis.password": "",
        "redis.timeout": "5000",
        "redis.db": "0",
        "redis.kcql": "INSERT INTO students- SELECT * FROM postgres.public.students PK id STOREAS HASH;INSERT INTO courses- SELECT * FROM postgres.public.courses PK id STOREAS HASH",
        "connect.redis.sink.kcql.prefix.keys": "true",
        "connect.redis.sink.expire.keys": "false",
        "connect.redis.sink.expire.keys.time.sec": "3600"
    }
} 